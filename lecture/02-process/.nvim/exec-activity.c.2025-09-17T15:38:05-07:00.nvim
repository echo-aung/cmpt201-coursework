#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>

int main() {
  pid_t pid = fork();
  printf("Successfully forked with pid=%d\n", pid);

  if (pid == 0) {
    printf("Parent in\n");
    execl("/usr/bin/ls", "/usr/bin/ls", "-a", (char *)NULL);

  } else {
    printf("Child in\n");
  }
}
